<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke android:width="@dimen/nl_sel_oval_outline_stroke_size"
                android:color="@color/nl_sel_oval_outline_normal"/>
            <solid android:color="@color/nl_transparent"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:width="@dimen/nl_sel_oval_outline_stroke_size"
                android:color="@color/nl_sel_oval_outline_pressed"/>
            <solid android:color="@color/nl_transparent"/>
        </shape>
    </item>
</selector>
