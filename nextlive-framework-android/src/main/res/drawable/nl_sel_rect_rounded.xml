<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/nl_sel_rect_rounded_normal"/>
            <corners android:radius="@dimen/nl_sel_rect_rounded_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/nl_sel_rect_rounded_pressed"/>
            <corners android:radius="@dimen/nl_sel_rect_rounded_radius" />
        </shape>
    </item>
</selector>
